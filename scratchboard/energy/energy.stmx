	<default_format/>
	<data>
		<import isee:overwrite="false" isee:timevarying="true" isee:interpolate="f" orientation="horizontal" resource="r../MINDinvestementsBAU.csv"/>
	</data>
	<model_units>
		<unit name="Dimensionless">
			<eqn>1</eqn>
			<alias>dmnl</alias>
			<alias>unitless</alias>
			<alias>fraction</alias>
		</unit>
		<unit name="$">
			<eqn/>
			<alias>dollar</alias>
			<alias>dollars</alias>
		</unit>
		<unit name="Hours">
			<eqn/>
			<alias>hr</alias>
			<alias>hour</alias>
		</unit>
		<unit name="Dollars Per Year">
			<eqn>$/Years</eqn>
			<alias>Dollar Per Year</alias>
			<alias>$ per year</alias>
		</unit>
		<unit name="Years">
			<eqn/>
			<alias>yr</alias>
			<alias>year</alias>
		</unit>
	</model_units>
	<model>
		<variables>
			<aux name="Secondary Fossil Energy Output">
				<eqn>Total_Factor_Productivity_Fossil_Energy*(
share_of_primary_energy_in_fossil_energy*
(Primary_Fossil_Energy_Scaling_Parameter*&quot;Primary_Fossil_Energy_(FUEL)&quot;)^
(Elasticity_of_Substitution_in_Fossil_Energy_Output)
+
(1-share_of_primary_energy_in_fossil_energy) *
(Fossil_Energy_Capital_Scaling_Parameter*Fossil_Energy_Capital)^(Elasticity_of_Substitution_in_Fossil_Energy_Output)
)^(1/Elasticity_of_Substitution_in_Fossil_Energy_Output)</eqn>
				<units>zJ/Year</units>
			</aux>
			<stock name="Fossil Energy Capital">
				<eqn>initial_fossil_energy_capital</eqn>
				<inflow>Investment_in_Fossil_Energy_Capital</inflow>
				<outflow>Depreciation_of_Fossil_Energy_Capital</outflow>
				<non_negative/>
				<units>$</units>
			</stock>
			<flow name="Investment in Fossil\nEnergy Capital">
				<eqn>Investments_in_Fossil_Energy_Capital</eqn>
				<non_negative/>
				<units>Dollars Per Year</units>
			</flow>
			<aux name="depreciation rate of fossil energy capital">
				<eqn>0.05</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Total Factor Productivity\nFossil Energy">
				<eqn>3.85000</eqn>
				<units>zJ/Year</units>
			</aux>
			<aux name="share of primary energy in fossil energy">
				<eqn>0.5</eqn>
				<units>Dimensionless</units>
			</aux>
			<stock name="Extracted Fossil\nEnergy Resources">
				<eqn>initial_extracted_fossil_resources</eqn>
				<inflow>Fossil_Energy_Extraction</inflow>
				<non_negative/>
				<units>gtC</units>
			</stock>
			<flow name="Fossil Energy Extraction">
				<eqn>Fossil_Fuel_Extraction_Capital*Fossil_Fuel_Extraction_Productivity</eqn>
				<non_negative/>
				<units>gtC/Year</units>
			</flow>
			<aux name="Primary Fossil Energy (FUEL)">
				<eqn>Fossil_Energy_Extraction*Carbon_Intensity</eqn>
				<units>zJ/Year</units>
			</aux>
			<aux name="Carbon Intensity">
				<doc>initial (1995) primary energy per unit of carbon projected into future [ZJ per GTC];

Calculate conversion factor of extracted carbon to primary fossil energy

pef     = 0.320;

m0(t)   = pef/quresex0 ;

* Data based on Marland et al., CDIAC, 2006  (cement manufacture included)
* Link: http://cdiac.ornl.gov/trends/emis/tre_glob.htm

quresex0 = 6.402;</doc>
				<eqn>0.0499844</eqn>
				<units>zJ/gtC</units>
			</aux>
			<aux name="Primary Fossil Energy\nScaling Parameter">
				<eqn>0.18750</eqn>
				<units>Year/zJ</units>
			</aux>
			<aux name="Elasticity of Substitution\nin Fossil Energy Output">
				<eqn>-2.33333</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="Resource base">
				<eqn>3500</eqn>
				<units>gtC</units>
			</aux>
			<aux name="Fossil Fuel Extraction\nScarcity Effect">
				<eqn>Initial_extraction_costs/Marginal_Extraction_Costs</eqn>
				<units>Dimensionless</units>
			</aux>
			<stock name="Fossil Fuel Extraction Capital">
				<eqn>initial_fossil_fuel_extraction_capital</eqn>
				<inflow>Investment_in_Fossil_Fuel_Extraction_Capital</inflow>
				<outflow>Depreciation_of_Fuel_Extraction_Capital</outflow>
				<non_negative/>
				<units>$</units>
			</stock>
			<aux name="Fossil Fuel Extraction Productivity">
				<eqn>Fossil_Fuel_Extraction_Scarcity_Effect*Fossil_Extraction_Accumulated_Knowledge*knowledgePerProductivity</eqn>
				<units>gtC/$/Year</units>
			</aux>
			<aux name="Initial extraction\ncosts">
				<doc>Initial extraction costs [$ per tC]
but used as is in the GAMS code even though other units in that equation are in gtC</doc>
				<eqn>113</eqn>
				<units>$/gtC</units>
			</aux>
			<aux name="Marginal Extraction Costs">
				<eqn>Initial_extraction_costs+
Slope_of_cost_curve*
(Extracted_Fossil_Energy_Resources/Resource_base)^Exponent_of_cost_curve</eqn>
				<units>$/gtC</units>
			</aux>
			<aux name="Slope of cost curve">
				<doc>Slope of cost curve [$ per tC],
but used as is in the GAMS code even though other units in that equation are in gtC</doc>
				<eqn>700</eqn>
				<units>$/gtC</units>
			</aux>
			<aux name="maximum increase of capital\ncoefficient in fossil\nresource sector">
				<eqn>256.08</eqn>
				<units>extractionknowledge</units>
			</aux>
			<stock name="Fossil Extraction\nAccumulated Knowledge">
				<eqn>initial_fossil_extraction_knowledge</eqn>
				<inflow>Change_in_Fossil_Learning_by_Doing_Effect</inflow>
				<non_negative/>
				<units>extractionknowledge</units>
			</stock>
			<flow name="Change in Fossil Learning by Doing Effect">
				<eqn>Fossil_Extraction_Accumulated_Knowledge/(learning_time_in_resource_sector*maximum_increase_of_capital_coefficient_in_fossil_resource_sector) *
(maximum_increase_of_capital_coefficient_in_fossil_resource_sector-Fossil_Extraction_Accumulated_Knowledge)*
((Fossil_Energy_Extraction/initialFossilEnergyExtraction)^learning_elasticity_of_the_resource_sector -1)</eqn>
				<units>extractionknowledge/Year</units>
			</flow>
			<aux name="learning time\nin resource sector">
				<eqn>1/0.01</eqn>
				<units>Year</units>
			</aux>
			<aux name="initial extracted fossil resources">
				<doc>* Data based on Marland et al., CDIAC, 2006  (cement manufacture included)
* Link: http://cdiac.ornl.gov/trends/emis/tre_glob.htm
1995</doc>
				<eqn>0</eqn>
				<units>gtC</units>
			</aux>
			<aux name="learning elasticity of\nthe resource sector">
				<eqn>0.4</eqn>
				<units>Dimensionless</units>
			</aux>
			<flow name="Depreciation of\nFossil Energy Capital">
				<eqn>Fossil_Energy_Capital*depreciation_rate_of_fossil_energy_capital</eqn>
				<non_negative/>
				<units>Dollars Per Year</units>
			</flow>
			<flow name="Depreciation of Fuel Extraction Capital">
				<eqn>Depreciation_rate_of_Fuel_Extraction_Capital*Fossil_Fuel_Extraction_Capital</eqn>
				<non_negative/>
				<units>Dollars Per Year</units>
			</flow>
			<aux name="Depreciation rate of Fuel Extraction Capital">
				<eqn>0.05</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Exponent of cost curve">
				<eqn>2</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="initial fossil fuel extraction capital">
				<eqn>0.05</eqn>
				<units>$</units>
			</aux>
			<flow name="Investment in Fossil\nFuel Extraction Capital">
				<eqn>Investments_in_Fossil_Fuel_Extraction_Capital</eqn>
				<non_negative/>
				<units>Dollars Per Year</units>
			</flow>
			<aux name="Renewable Energy Output">
				<doc>0.0000000036 is gwh2zj</doc>
				<eqn>Renewable_Energy_Full_Load_Hours * 
Total_Renewable_Energy_Capacity * gWh_to_zJ</eqn>
				<units>zJ/Years</units>
			</aux>
			<aux name="Renewable Energy Full Load Hours">
				<eqn>2190</eqn>
				<units>hours/year</units>
			</aux>
			<aux name="Total Energy Output">
				<eqn>Secondary_Fossil_Energy_Output+Renewable_Energy_Output</eqn>
				<units>zJ/Year</units>
			</aux>
			<stock name="Renewable Energy Installed Capacity">
				<dimensions>
					<dim name="Renewable_Age_Group"/>
				</dimensions>
				<eqn>Initial_Installed_Renewable_Energy_Capacity</eqn>
				<inflow>Renewable_Energy_Capacity_Construction</inflow>
				<inflow>Renewable_Energy_Capacity_Vintage_Aging_In</inflow>
				<outflow>Renewable_Energy_Capacity_Vintage_Aging_Out</outflow>
				<outflow>Renewable_Energy_Capacity_Decay</outflow>
				<conveyor fifo="true">
					<len>Renewable_Energy_Capacity_Vintage_Duration</len>
				</conveyor>
				<units>gW</units>
			</stock>
			<flow name="Renewable Energy\nCapacity Construction">
				<dimensions>
					<dim name="Renewable_Age_Group"/>
				</dimensions>
				<eqn>IF(Renewable_Age_Group=1)THEN 
kilo_to_giga *
Productivity_of_Renewable_Energy_Investments*
tera_to_non *
1/monetary_scale *
Investments_in_Renewable_Energy_Capcity_in_Money 
ELSE 0</eqn>
				<non_negative/>
				<units>gW/Year</units>
			</flow>
			<flow name="Renewable Energy Capacity Vintage Aging Out">
				<dimensions>
					<dim name="Renewable_Age_Group"/>
				</dimensions>
				<eqn>0</eqn>
				<non_negative/>
				<units>gW/Year</units>
			</flow>
			<flow name="Renewable Energy Capacity Vintage Aging In">
				<dimensions>
					<dim name="Renewable_Age_Group"/>
				</dimensions>
				<eqn>Renewable_Energy_Capacity_Vintage_Aging_Out[Renewable_Age_Group-1]</eqn>
				<non_negative/>
				<units>gW/Year</units>
			</flow>
			<aux name="Total Renewable Energy Capacity">
				<eqn>SUM(Renewable_Energy_Installed_Capacity)</eqn>
				<units>gW</units>
			</aux>
			<flow name="Renewable Energy\nCapacity Decay">
				<dimensions>
					<dim name="Renewable_Age_Group"/>
				</dimensions>
				<eqn>(Renewable_Energy_Capacity_Decay_Fractions[Renewable_Age_Group])/Renewable_Energy_Capacity_Vintage_Duration</eqn>
				<non_negative/>
				<leak/>
				<units>gW/Year</units>
			</flow>
			<aux name="Renewable Energy Capacity Decay Fractions">
				<dimensions>
					<dim name="Renewable_Age_Group"/>
				</dimensions>
				<element subscript="1">
					<eqn>0</eqn>
				</element>
				<element subscript="2">
					<eqn>0.1</eqn>
				</element>
				<element subscript="3">
					<eqn>1-0.8/0.9</eqn>
				</element>
				<element subscript="4">
					<eqn>1-0.5/0.7</eqn>
				</element>
				<element subscript="5">
					<eqn>1-0.15/0.5</eqn>
				</element>
				<element subscript="6">
					<eqn>1-0.05/0.5</eqn>
				</element>
				<element subscript="7">
					<eqn>1</eqn>
				</element>
				<units>Dimensionless</units>
			</aux>
			<aux name="Renewable Energy Capacity\nVintage Duration">
				<eqn>5</eqn>
				<units>Years</units>
			</aux>
			<aux name="Investments in Renewable Energy Capcity in Money">
				<eqn>IF (TIME&lt;2010) THEN
0.005 
ELSE IF (TIME &lt; 2015) THEN
0.014
ELSE IF (TIME &lt; 2040) THEN
0.03
ELSE IF (TIME &lt; 2075) THEN
0.04
ELSE IF (TIME &lt; 2130) THEN
0.06
ELSE IF (TIME &lt; 2200) THEN
0.12
ELSE 0</eqn>
				<units>Dollars Per Year</units>
			</aux>
			<aux name="Productivity of\nRenewable Energy\nInvestments">
				<eqn>1/(Renewable_Energy_Floor_Costs+Renewable_Energy_Reducible_Installation_Costs)</eqn>
				<units>gW/$</units>
			</aux>
			<aux name="Renewable Energy\nReducible Installation\nCosts">
				<eqn>Initial_Renewable_Energy_Installation_Costs *
(SUM(Initial_Installed_Renewable_Energy_Capacity)/Cumulative_Renewable_Energy_Capacity_Construction) ^ 
(-LN(1-Renewable_Energy_Learning_Rate)/LN(2))</eqn>
				<units>$/gW</units>
			</aux>
			<aux name="Renewable Energy\nLearning Rate">
				<eqn>0.15</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="Renewable Energy\nFloor Costs">
				<eqn>500</eqn>
				<units>$/gW</units>
			</aux>
			<aux name="Initial Renewable Energy Installation Costs">
				<eqn>700</eqn>
				<units>$/gW</units>
			</aux>
			<stock name="Cumulative Renewable Energy Capacity Construction">
				<eqn>SUM(Initial_Installed_Renewable_Energy_Capacity)</eqn>
				<inflow>accumulating_Renewable_Energy_Construction</inflow>
				<non_negative/>
				<units>gW</units>
			</stock>
			<flow name="accumulating Renewable Energy Construction">
				<eqn>Renewable_Energy_Capacity_Construction[1]</eqn>
				<non_negative/>
				<units>gW/Year</units>
			</flow>
			<aux name="Initial Installed Renewable Energy Capacity">
				<dimensions>
					<dim name="Renewable_Age_Group"/>
				</dimensions>
				<element subscript="1">
					<eqn>684.1812817</eqn>
				</element>
				<element subscript="2">
					<eqn>251.8651181</eqn>
				</element>
				<element subscript="3">
					<eqn>92.4862276</eqn>
				</element>
				<element subscript="4">
					<eqn>33.9199610</eqn>
				</element>
				<element subscript="5">
					<eqn>12.4682687</eqn>
				</element>
				<element subscript="6">
					<eqn>4.5775583</eqn>
				</element>
				<element subscript="7">
					<eqn>2.5175186</eqn>
				</element>
				<units>gW</units>
			</aux>
			<aux name="Investments in Fossil\nEnergy Capital">
				<eqn>IF(TIME&lt;2015)THEN 0.004 ELSE 0.00001</eqn>
				<units>Dollars Per Year</units>
			</aux>
			<aux name="Investments in Fossil\nFuel Extraction Capital">
				<eqn>IF(TIME&lt;2015)THEN 0.004 ELSE 0.00001</eqn>
				<units>Dollars Per Year</units>
			</aux>
			<aux name="Fossil Energy Capital Scaling Parameter">
				<eqn>1</eqn>
				<units>1/$</units>
			</aux>
			<aux name="knowledgePerProductivity">
				<eqn>1</eqn>
				<units>gtC/$/Year/Extractionknowledge</units>
			</aux>
			<aux name="someFactor">
				<eqn>2</eqn>
				<units>$*extractionknowledge/gtC</units>
			</aux>
			<aux name="gWh to zJ">
				<eqn>3.6E-9</eqn>
				<units>zJ/(gW*hours)</units>
			</aux>
			<aux name="initial fossil energy capital">
				<eqn>0.06</eqn>
				<units>$</units>
			</aux>
			<aux name="initialFossilEnergyExtraction">
				<eqn>initial_fossil_fuel_extraction_capital*(Initial_extraction_costs/initial_marginal_extraction_costs)*initial_fossil_extraction_knowledge*knowledgePerProductivity</eqn>
				<units>gtC/Year</units>
			</aux>
			<aux name="initial fossil extraction knowledge">
				<eqn>128.04</eqn>
				<units>extractionknowledge</units>
			</aux>
			<aux name="initial marginal extraction costs">
				<eqn>Initial_extraction_costs+
Slope_of_cost_curve*
(initial_extracted_fossil_resources/Resource_base)^Exponent_of_cost_curve</eqn>
				<units>$/gtC</units>
			</aux>
			<aux name="monetary scale">
				<eqn>0.01</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="kilo to giga">
				<eqn>1e-6</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="tera to non">
				<eqn>1e+12</eqn>
				<units>dmnl</units>
			</aux>
		</variables>
