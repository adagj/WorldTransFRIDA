			<aux name="effect of\ncrop yield on fertilizer use\nper ha">
				<eqn>sensitivity_of_crop_yield_on_fertilizer_use_per_ha_alpha*(average_crop_yield/INIT(average_crop_yield))^sensitivity_of_crop_yield_on_fertilizer_use_per_ha_gamma</eqn>
				<units>dmnl</units>
			</aux>
