				<units>1</units>
			</aux>
			<aux name="UNIT kgN/tN">
				<eqn>1000</eqn>
				<units>kgN/tN</units>
			</aux>
			<aux name="normal rate of CO2 absorption in\nforestry land">
				<doc>Calibrated (CO2 emissions from land cover change)</doc>
				<eqn>1.6</eqn>
