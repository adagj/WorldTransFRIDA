				</connector>
				<alias uid="47" x="1122" y="515" width="18" height="18">
					<of>Observed_warming</of>
				</alias>
				<alias uid="108" x="149" y="1460" width="18" height="18">
					<of>Observed_warming</of>
				</alias>
				<alias font_style="italic" uid="130" x="228" y="1413" width="18" height="18">
					<of>Observed_warming_in_1980</of>
				</alias>
				<alias font_style="italic" uid="135" x="990" y="35" width="18" height="18">
					<of>Warming_in_1980</of>
				</alias>
			</view>
		</views>
	</model>
	<model name="Food_and_land_use">
		<variables>
			<aux name="Crop use">
				<eqn>Crop_supply</eqn>
				<units>Mtcrop/year</units>
			</aux>
			<aux name="Crop supply" access="output">
				<eqn>Average_crop_yield*Cropland</eqn>
				<units>Mtcrop/year</units>
			</aux>
			<aux name="Cost index for Regenerative\nagriculture">
				<eqn>(1-Cost_reduction_per_doubling_in_Regenerative_agriculture)^Number_of_doublings_in_reg_ag</eqn>
				<units>1</units>
			</aux>
			<aux name="Cost reduction per doubling in\nRegenerative agriculture">
				<doc>	was 0.1</doc>
				<eqn>0.05</eqn>
				<units>1</units>
			</aux>
			<aux name="Number of doublings\nin reg ag">
				<doc>	ln2 = 0.693</doc>
				<eqn>LN((Regenerative_agriculture_area+Experience_gained_before_2022)/Experience_gained_before_2022)/0.693</eqn>
				<units>1</units>
			</aux>
			<aux name="Experience gained\nbefore 2022">
				<eqn>5</eqn>
				<units>Mha</units>
			</aux>
			<aux name="Regenerative\nagriculture area">
				<eqn>Cropland*Fraction_regenerative_agriculture</eqn>
				<units>Mha</units>
			</aux>
			<aux name="Fertilizer use in 1980">
				<eqn>INIT(Fertilizer_use)</eqn>
				<units>MtN/year</units>
			</aux>
			<aux name="Food footprint in 1980">
				<eqn>Cropland_in_1980*Fertilizer_use_in_1980</eqn>
				<units>Mha*MtN/year</units>
			</aux>
			<aux name="Cropland in 1980">
				<doc>	Was 1350 Mha - From Earth3, but similar to HYDE - rising to 1500 in 2020
		211030: changed to 1420 from UG FAO</doc>
				<eqn>1268.75</eqn>
				<units>Mha</units>
			</aux>
			<aux name="FOOD FOOTPRINT INDEX (1980=1)">
				<eqn>Food_footprint/Food_footprint_in_1980</eqn>
				<units>1</units>
			</aux>
			<aux name="Food footprint">
				<eqn>Cropland*Fertilizer_use</eqn>
				<units>Mha*MtN/year</units>
			</aux>
			<stock name="Cropland">
				<doc>	From HYDE: Cropland 1.600</doc>
				<eqn>Cropland_in_1980</eqn>
				<inflow>Cropland_expansion</inflow>
				<outflow>Cropland_loss</outflow>
				<outflow>Urban_expansion</outflow>
				<units>Mha</units>
			</stock>
			<aux name="Crop use per person" access="output" isee:autocreated="true">
				<eqn>Crop_use/Population_Mp</eqn>
				<units>tcrop/(p*Year)</units>
			</aux>
			<aux name="CO2 absorption in\nforestry land">
				<doc>	Numbers for Northern forest from ESMICON 2016: 0.24 GtC/y * 3.7 tCO2/tC / 1100 Mha = 0.8 tCO2/ha/y
		Increased by 100% to include absorption in non-Northern forests</doc>
				<eqn>Rate_of_CO2_absorption_in_forestry_land</eqn>
				<units>tCO2/(ha*year)</units>
			</aux>
			<aux name="Old growth removal">
				<eqn>Old_growth_forest_area*Old_growth_removal_rate</eqn>
				<units>Mha/year</units>
			</aux>
			<stock name="Old growth forest area">
				<eqn>Old_growth_forest_in_1980</eqn>
				<outflow>New_forestry_land</outflow>
				<outflow>&quot;New_grazing_land_Mha/y&quot;</outflow>
				<units>Mha</units>
			</stock>
			<aux name="Old growth removal rate">
				<eqn>OGRR_in_1980*FFLReoOGRR</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Land erosion rate">
				<eqn>Land_erosion_rate_in_1980*Fertilizer_effect_on_erosion_rate</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Land erosion rate in 1980">
				<doc>	equal to 1 / 250 years</doc>
				<eqn>0.004</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Fertilizer effect\non erosion rate">
				<eqn>1+&quot;sFUeoLER&gt;0&quot;*(Fertilizer_use_in_conv_ag/Sustainable_fertiliser_use-1)</eqn>
				<units>1</units>
			</aux>
			<flow name="Cropland loss">
				<eqn>Cropland*Land_erosion_rate</eqn>
				<units>Mha/Year</units>
			</flow>
			<aux name="Grazing land yied in\n1980">
				<doc>	Estimate derived from complex considerations. Should be between 10 and 30 kg red meat/ha/y in US grazing lands</doc>
				<eqn>Grazing_land_yied_in_1980_before_multiplication*CO2_effect_on_land_yield*Warming_effect_on_land_yield</eqn>
				<units>(kg redmeat)/(ha*year)</units>
			</aux>
			<aux name="CO2 effect on land yield">
				<eqn>( 1+&quot;sCO2CeoACY&gt;0&quot;*(CO2_concentration_in_atm_ppm/CO2_concentration_in_1980-1))</eqn>
				<units>1</units>
			</aux>
			<aux name="Average crop yield">
				<eqn>(Desired_crop_yield_in_conv_ag*(1-Fraction_regenerative_agriculture)*&quot;Soil_quality_index_in_conv_ag_(1980=1)&quot;+Crop_yield_in_reg_ag*Fraction_regenerative_agriculture)*CO2_effect_on_land_yield*Warming_effect_on_land_yield</eqn>
				<units>tcrop/(ha*year)</units>
			</aux>
			<aux name="CO2 absorption in forestry land GtCO2/y">
				<eqn>Forestry_land*(CO2_absorption_in_forestry_land/&quot;UNIT_MtCO2/GtCO2&quot;)*CO2_effect_on_land_yield*Warming_effect_on_land_yield</eqn>
				<units>GtCO2/year</units>
			</aux>
			<stock name="Forestry land">
				<eqn>Forestry_land_in_1980</eqn>
				<inflow>New_forestry_land</inflow>
				<outflow>Cropland_expansion</outflow>
				<units>Mha</units>
			</stock>
			<aux name="Desired crop yield in\nconv ag">
				<eqn>Desired_crop_supply_conv_ag/(Cropland*(1-Fraction_regenerative_agriculture))</eqn>
				<units>tcrop/(ha*year)</units>
			</aux>
			<stock name="Soil quality index in conv ag (1980=1)">
				<eqn>Soil_quality_index_in_1980</eqn>
				<inflow>Change_in_soil_quality_in_conv_ag</inflow>
				<units>1</units>
			</stock>
			<aux name="Crop yield in reg\nag">
				<doc>	5 (reduced from 7 on 220427 to get acceptable fertilizer forecast)</doc>
				<eqn>5</eqn>
				<units>tcrop/(ha*year)</units>
			</aux>
			<aux name="Desired crop supply">
				<eqn>Crop_demand</eqn>
				<units>Mtcrop/year</units>
			</aux>
			<aux name="Crop demand">
				<eqn>(Traditional_use_of_crops_ex_red_meat+Feed_for_red_meat+Crops_for_biofuel)</eqn>
				<units>Mtcrop/year</units>
			</aux>
			<aux name="Crop balance">
				<eqn>Crop_use/Desired_crop_supply</eqn>
				<units>1</units>
			</aux>
			<flow name="Cropland expansion">
				<eqn>( IF Forestry_land &gt; 0 THEN Cropland*Cropland_expansion_rate ELSE 0 )*Acceptable_loss_of_forestry_land</eqn>
				<units>Mha/Year</units>
			</flow>
			<aux name="Cropland expansion rate">
				<eqn>(1/200+&quot;sFBeoCLE&lt;0&quot;*(Perceived_crop_balance-1))/time_for_percieved_crop_balance_to_impact_cropland</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Acceptable loss of\nforestry land">
				<eqn>1-EXP(-Fraction_forestry_land_remaining/Threshold_FFLR)</eqn>
				<units>1</units>
			</aux>
			<aux name="Total forest area">
				<eqn>Old_growth_forest_area+Forestry_land</eqn>
				<units>Mha</units>
			</aux>
			<aux name="Fraction forestry\nland remaining">
				<eqn>MAX(0, Forestry_land/Forestry_land_in_1980)</eqn>
				<units>1</units>
			</aux>
			<aux name="Threshold FFLR">
				<doc>	Parametrized to ensure there is still some forestry land in 2100 in Base Case</doc>
				<eqn>0.4</eqn>
				<units>1</units>
			</aux>
			<aux name="Forestry land in 1980">
				<doc>	From HYDE: Woodlands 3500 minus Wild remote 1550 and seminatural woodlands remote 850</doc>
				<eqn>1100</eqn>
				<units>Mha</units>
			</aux>
			<aux name="Food sector productivity\nindex (1980=1)">
				<eqn>EXP(ROC_in_food_sector_productivity*(TIME-1980))*( IF TIME &gt; 2022 THEN EXP(Extra_ROC_in_food_sector_productivity_from_2022*(TIME-2022)) ELSE 1 )</eqn>
				<units>1</units>
			</aux>
			<aux name="ROC in food sector\nproductivity">
				<doc>	0.002 chosen to fit SSP2 4-5</doc>
				<eqn>0.002</eqn>
				<units>1/y</units>
			</aux>
			<aux name="Extra ROC in food sector\nproductivity from 2022">
				<doc>	0 in TLTL and 0 in GL. SInce ROC in food sector is already 0.002 1/y</doc>
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<aux name="CO2 emissions from\nLULUC" access="output">
				<eqn>CO2_release_from_forest_cut-&quot;CO2_absorption_in_forestry_land_GtCO2/y&quot;-Extra_CO2_absorption_in_reg_ag</eqn>
				<units>GtCO2/year</units>
			</aux>
			<aux name="Fraction regenerative\nagriculture">
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<aux name="Fraction new red meat">
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<aux name="Fertilizer use\nper person">
				<eqn>(Fertilizer_use/Population_Mp)*&quot;UNIT_kgN/tN&quot;</eqn>
				<units>kgN/(p*Years)</units>
			</aux>
			<aux name="Fertilizer use" access="output">
				<eqn>Cropland*(1-Fraction_regenerative_agriculture)*Fertilizer_use_in_conv_ag/&quot;UNIT_kgN/tN_1&quot;</eqn>
				<units>MtN/year</units>
			</aux>
			<aux name="Demand for red\nmeat">
				<eqn>((Population_Mp*Demand_for_red_meat_per_person)/&quot;UNIT_kg_red_meat/ton_read_meat&quot;)*(1-Fraction_new_red_meat)</eqn>
				<units>Mt redmeat/Years</units>
			</aux>
			<aux name="Demand for red meat per\nperson">
				<eqn>Traditional_use_of_red_meat_per_person</eqn>
				<units>(kg redmeat)/(p*year)</units>
			</aux>
			<aux name="Extra cost of reg ag">
				<eqn>Extra_cost_of_reg_ag_in_2022*Cost_index_for_Regenerative_agriculture</eqn>
				<units>$/(ha*year)</units>
			</aux>
			<aux name="Extra cost of reg\nag in 2022">
				<doc>	I assume that reg ag requires 30 % higher manpower input, and that this increases total farming cost by 30%.
		Current manpower input in Norway is 20.000 fulltime farmers cultivating 1 Mha =&gt; 0.02 p/ha/y. At 600.000 kr/p/y this means an extra cost of 30%*600.000 kr/p/y*0.02 p/ha/y = 3600 kr/p/y or 400$/ha/y</doc>
				<eqn>400</eqn>
				<units>$/(ha*year)</units>
			</aux>
			<aux name="Extra cost of Food Turnaround\nas share of GDP">
				<eqn>Extra_cost_of_Food_Turnaround/GDP</eqn>
				<units>1</units>
			</aux>
			<aux name="Extra cost of Food\nTurnaround">
				<eqn>Cost_of_regenerative_agriculture-Fertilizer_cost_reduction</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Amount of fertilizer\nsaved in reg ag">
				<doc>	Comment: From the table function I know that 268 kgN/ha/y fertilizer is needed to achieve the yield in regenerative agriculture (namely 7 tcrop/ha/y). I assume that reg ag reduces this need to the sustainable use 20 kgN/ha/y</doc>
				<eqn>268-Sustainable_fertiliser_use</eqn>
				<units>kgN/(ha*year)</units>
			</aux>
			<aux name="Sustainable fertiliser\nuse">
				<eqn>20</eqn>
				<units>kgN/(ha*year)</units>
			</aux>
			<aux name="Fertilizer cost reduction">
				<eqn>(Amount_of_fertilizer_saved_in_reg_ag/&quot;UNIT_kgN/tN&quot;)*Regenerative_agriculture_area*(Cost_per_ton_fertilizer)/&quot;Unit_M$/G$&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Traditional use of red meat\nper person">
				<doc>	Equal to traditional use growing asymptotically towards ca 40 kg/p/y, minus &quot;automatic&quot; red-meat-use-index falling from 100% at 40 k$/p/y to 70% at 100 k$/p/y as a consequence of more educated eating habits</doc>
				<eqn>&quot;GDP_per_person_k$/p/y&quot;</eqn>
				<gf>
					<yscale min="0" max="33"/>
					<xpts>0,6.1,8.8,14,30,40,50,100</xpts>
					<ypts>0,6,8.5,13,27,32,33,25</ypts>
				</gf>
				<units>(kg redmeat)/(p*year)</units>
			</aux>
			<aux name="Traditional use of\ncrops ex red meat">
				<eqn>(Traditional_use_of_crops-Traditional_use_of_feed_for_red_meat)/&quot;Food_sector_productivity_index_(1980=1)&quot;</eqn>
				<units>Mtcrop/year</units>
			</aux>
			<aux name="Traditional use of crops">
				<eqn>(Traditional_use_of_crops_per_person*Population_Mp)/&quot;UNIT_(kg_crop)/(t_crop)&quot;</eqn>
				<units>Mtcrop/year</units>
			</aux>
			<aux name="Traditional use of feed\nfor red meat">
				<eqn>(((Traditional_use_of_red_meat_per_person/&quot;UNIT_kg_red_meat/ton_read_meat&quot;)*Population_Mp)-Red_meat_from_grazing_land)*&quot;UNIT_tcrop/t_redmeat&quot;</eqn>
				<units>Mtcrop/year</units>
			</aux>
			<aux name="Red meat from feedlots">
				<eqn>Demand_for_red_meat-Red_meat_from_grazing_land</eqn>
				<units>Mt redmeat/Years</units>
			</aux>
			<aux name="Red meat from grazing\nland">
				<eqn>MIN(Demand_for_red_meat, Potential_red_meat_from_grazing_land)</eqn>
				<units>Mt redmeat/Years</units>
			</aux>
			<aux name="sFUeoLER&gt;0">
				<doc>	GUSTIMATE: 100 kgNP/ha/y halves cropland life from 250 to 125 years</doc>
				<eqn>0.02</eqn>
				<units>1</units>
			</aux>
			<aux name="Fertilizer use in\nconv ag">
				<eqn>Traditional_fertilizer_use_in_conv_ag/&quot;Fertilizer_productivity_index_(1980=1)&quot;</eqn>
				<units>kgN/(ha*year)</units>
			</aux>
			<aux name="Potential red meat from\ngrazing land">
				<eqn>(Grazing_land*Grazing_land_yield)/&quot;UNIT_kg_red_meat/ton_read_meat&quot;</eqn>
				<units>Mt redmeat/Years</units>
			</aux>
			<aux name="Feed for red meat">
				<eqn>Red_meat_from_feedlots*&quot;UNIT_tcrop/t_redmeat&quot;</eqn>
				<units>Mtcrop/year</units>
			</aux>
			<aux name="Crops for biofuel">
				<eqn>Biofuels_use*Ton_crops_per_toe_biofuel</eqn>
				<units>Mtcrop/year</units>
			</aux>
			<aux name="ROC in fertilizer\nproductivity">
				<eqn>0.01</eqn>
				<units>1/year</units>
			</aux>
			<aux name="ROC in soil quality\nin conv ag">
				<eqn>0+&quot;sFUeoSQ&lt;0&quot;*(Fertilizer_use_in_conv_ag/Sustainable_fertiliser_use-1)</eqn>
				<units>1/year</units>
			</aux>
			<aux name="sFUeoSQ&lt;0">
				<doc>	was -0.001, so -0.0005, so 0, so 0.0005, so -0.0002</doc>
				<eqn>-0.001</eqn>
				<units>1/yr</units>
			</aux>
			<aux name="Fertilizer productivity\nindex (1980=1)">
				<eqn>EXP(ROC_in_fertilizer_productivity*(TIME-1980))</eqn>
				<units>1</units>
			</aux>
			<aux name="Traditional fertilizer\nuse in conv ag">
				<doc>	211030: Changed to fit UG numbers from FAO</doc>
				<eqn>Desired_crop_yield_in_conv_ag</eqn>
				<gf>
					<yscale min="0" max="600"/>
					<xpts>1,2,2.5,3,3.5,4.5,6.5,10</xpts>
					<ypts>0,40,50,60,70,100,200,600</ypts>
				</gf>
				<units>kgN/ha/year</units>
			</aux>
			<aux name="Grazing land yield">
				<eqn>Grazing_land_yied_in_1980</eqn>
				<units>(kg redmeat)/(ha*year)</units>
			</aux>
			<aux name="sFBeoCLE&lt;0">
				<doc>	was 0.1, so 0.01,0.015, 0.02, 0.03, 0.005, - 0.004, so -0.04, so -0.05
		JR guestimate, based on fact that cropland has only expanded by some 10% since 1980</doc>
				<eqn>-0.03</eqn>
				<units>1</units>
			</aux>
			<aux name="Perceived crop balance">
				<eqn>Crop_balance/(1+Desired_reserve_capacity)</eqn>
				<units>1</units>
			</aux>
			<aux name="Biofuels use">
				<doc>	was ([(1980,0)-(2100,200)],(1980,0),(1990,0),(2000,2.5),(2020,40),(2100,40) )</doc>
				<eqn>TIME</eqn>
				<gf>
					<yscale min="0" max="1"/>
					<xpts>1980,1990,2000,2020,2100</xpts>
					<ypts>0,0,0,0,0</ypts>
				</gf>
				<units>Mtoe/year</units>
			</aux>
			<aux name="Desired reserve capacity">
				<doc>	0.1 sounds more reasonable</doc>
				<eqn>0.05</eqn>
				<units>1</units>
			</aux>
			<aux name="Red meat supply per\nperson">
				<eqn>(Red_meat_from_grazing_land+Red_meat_from_feedlots*MIN(1, Crop_balance))*&quot;UNIT_kg_red_meat/ton_read_meat&quot;/Population_Mp</eqn>
				<units>(kg redmeat)/(p*year)</units>
			</aux>
			<aux name="Desired crop supply\nconv ag">
				<eqn>Desired_crop_supply-Crop_supply_reg_ag</eqn>
				<units>Mtcrop/year</units>
			</aux>
			<aux name="Crop supply reg ag">
				<eqn>Crop_yield_in_reg_ag*Cropland*Fraction_regenerative_agriculture</eqn>
				<units>Mtcrop/year</units>
			</aux>
			<aux name="Traditional use of crops ex\nred meat per person">
				<eqn>Traditional_use_of_crops_ex_red_meat*&quot;UNIT_(kg_crop)/(t_crop)&quot;/Population_Mp</eqn>
				<units>(kg crop)/(p*year)</units>
			</aux>
			<aux name="Traditional use of crops\nper person">
				<doc>	211029: Estimated from UG FAO. Disregarding biofuel use growing from 0 in 2000 to 200 in 2020.</doc>
				<eqn>&quot;GDP_per_person_k$/p/y&quot;</eqn>
				<gf>
					<yscale min="400" max="1550"/>
					<xpts>0,6.1,8.7,13.9,20,30,40,60,100</xpts>
					<ypts>400,680,780,950,1050,1150,1250,1350,1550</ypts>
				</gf>
				<units>(kg crop)/(p*year)</units>
			</aux>
			<aux name="kg-crop per kg-red-meat">
				<doc>	cattle 25, lamb 15, pork 6.4, poultry 3.3 (OWID)
		One third each of 1) red meat (90% cattle, 10% lamb), 2) pork and 3) poultry</doc>
				<eqn>24</eqn>
				<units>1</units>
			</aux>
			<aux name="Ton crops per toe biofuel">
				<doc>	I think it should be around 4</doc>
				<eqn>0</eqn>
				<units>tcrop/toe</units>
			</aux>
			<aux name="Indicated urban land">
				<eqn>Population_Mp*Urban_land_per_population</eqn>
				<units>Mha</units>
			</aux>
			<stock name="Urban land">
				<eqn>Urban_land_in_1980</eqn>
				<inflow>Urban_expansion</inflow>
				<units>Mha</units>
			</stock>
			<aux name="Urban development time">
				<doc>	should be longer if we want to test urbanisation policies</doc>
				<eqn>10</eqn>
				<units>year</units>
			</aux>
			<flow name="Change in soil quality\nin conv ag">
				<eqn>ROC_in_soil_quality_in_conv_ag*&quot;Soil_quality_index_in_conv_ag_(1980=1)&quot;</eqn>
				<units>1/year</units>
			</flow>
			<aux name="Urban land per population">
				<doc>	From HYDE: 215, 285, 320 in 1980, 2000, 2017
		Means 0.049, 0.048, 0.041 ha/p ie around 500 m2 per person or 200 p/km2</doc>
				<eqn>0.05</eqn>
				<units>ha/p</units>
			</aux>
			<stock name="Barren land">
				<doc>	From HYDE: Barren land 3.000 Mha i 2017</doc>
				<eqn>3000</eqn>
				<inflow>Cropland_loss</inflow>
				<units>Mha</units>
			</stock>
			<aux name="Old growth forest in 1980">
				<doc>	From Earth3: Sum of Northern forest 1000 (slowly growing at 0.3 %/y) and Southern forest 1600 (declinig at 1 %/y)
		Similar to HYDE: Sum of Wild remote woodlands 1550 plus Seminatural remote woodlands 850</doc>
				<eqn>2600</eqn>
				<units>Mha</units>
			</aux>
			<stock name="Grazing land">
				<eqn>Grazing_land_in_1980</eqn>
				<inflow>&quot;New_grazing_land_Mha/y&quot;</inflow>
				<units>Mha</units>
			</stock>
			<aux name="Soil quality index in 1980">
				<eqn>1</eqn>
				<units>1</units>
			</aux>
			<aux name="Grazing land in 1980">
				<doc>	OWID and HYDE. Disregarding Rangeland 2.200,Pasture 0.8, forest converted to rangeland 300</doc>
				<eqn>3300</eqn>
				<units>Mha</units>
			</aux>
			<aux name="Urban land in 1980">
				<doc>	From HYDE: 215, 285, 320 in 1980, 2000, 2017
		Means 0.049, 0.048, 0.041 ha/p ie around 500 m2 per person or 200 p/km2</doc>
				<eqn>215</eqn>
				<units>Mha</units>
			</aux>
			<flow name="New forestry land">
				<eqn>Old_growth_removal*(1-Fraction_cleared_for_grazing)</eqn>
				<units>Mha/Year</units>
			</flow>
			<aux name="Fraction cleared\nfor grazing">
				<doc>	Since grazing land data show a stable area, forest must have been removed (ca 10 Mha/y) to increase cropland (soya?) (+7 Mha/y?)and to replace cropland loss (3 Mha/y?).</doc>
				<eqn>0.1</eqn>
				<units>1</units>
			</aux>
			<flow name="New grazing land Mha/y">
				<eqn>Old_growth_removal*Fraction_cleared_for_grazing</eqn>
				<units>Mha/Year</units>
			</flow>
			<aux name="sOWeoACY&lt;0">
				<doc>	I assume that yields decrease slowly with rising temperatures after 2022, but should accelerate.</doc>
				<eqn>-0.068978722284866</eqn>
				<units>1</units>
			</aux>
			<aux name="sCO2CeoACY&gt;0">
				<doc>	I assume that yields increase in proportion with CO2 ppm (ie s=1) after 2022, but should have been at a a declining pace</doc>
				<eqn>0.1</eqn>
				<units>1</units>
			</aux>
			<aux name="Cost of food" access="output">
				<eqn>Agriculture_as_fraction_of_GDP*GDP+Cost_of_regenerative_agriculture+Cost_of_fertilizer</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Loss of cropland">
				<eqn>Cropland_loss+Urban_expansion</eqn>
				<units>Mha/year</units>
			</aux>
			<flow name="Urban expansion">
				<eqn>MAX(0, (Indicated_urban_land-Urban_land)/Urban_development_time)</eqn>
				<units>Mha/Year</units>
			</flow>
			<aux name="Loss of forest land">
				<eqn>Old_growth_removal+Cropland_expansion</eqn>
				<units>Mha/year</units>
			</aux>
			<aux name="Cost of fertilizer">
				<eqn>Fertilizer_use*Cost_per_ton_fertilizer/&quot;Unit_M$/G$&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Cost per ton fertilizer">
				<doc>	Taken from Norway 2020 3000 - 5000 kr pr ton fertilizer</doc>
				<eqn>500</eqn>
				<units>$/tN</units>
			</aux>
			<aux name="Agriculture as\nfraction of GDP">
				<eqn>0.05</eqn>
				<units>1</units>
			</aux>
			<aux name="Cost of regenerative\nagriculture">
				<eqn>(Extra_cost_of_reg_ag*Regenerative_agriculture_area)/&quot;Unit_M$/G$&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Cost of food and energy\nas share of GDP">
				<eqn>Cost_of_food_and_energy/GDP</eqn>
				<units>1</units>
			</aux>
			<aux name="Cost of food and energy">
				<eqn>Cost_of_food+&quot;Cost_of_energy_G$/y&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Extra CO2 absorption\nin reg ag">
				<eqn>Regenerative_agriculture_area*CO2_absorbed_in_reg_ag/&quot;UNIT_MtCO2/GtCO2&quot;</eqn>
				<units>GtCO2/year</units>
			</aux>
			<aux name="CO2 absorbed in\nreg ag">
				<doc>	pure guess, but this is approx what is photosyntesized per ha per year</doc>
				<eqn>1</eqn>
				<units>tCO2/ha/year</units>
			</aux>
			<aux name="CO2 release from\nforest cut">
				<eqn>(Old_growth_removal*CO2_release_per_ha_of_old_growth_forest_cut+Cropland_expansion*CO2_release_per_ha_of_new_forest_cut)/&quot;UNIT_MtCO2/GtCO2&quot;</eqn>
				<units>GtCO2/year</units>
			</aux>
			<aux name="CO2 release per ha of old growth\nforest cut">
				<doc>	Numbers for Northern forests from ESMICON 2016: (215 GtC * 3.7 tCO2/tC ) / 1100 Mha = 72 tCO2/ha
		Numbers for Southern forests from ESMICON 2016: (260 GtC * 3.7 tCO2/tC ) / 1600 Mha = 60 tCO2/ha</doc>
				<eqn>585</eqn>
				<units>tCO2/ha</units>
			</aux>
			<aux name="sFFLReoOGRR&lt;0">
				<doc>	Parametrized to ensure there is still some forestry land in 2100 in Base Case</doc>
				<eqn>-5</eqn>
				<units>1</units>
			</aux>
			<aux name="OGRR in 1980">
				<doc>	0.4 % per year is a rough estimate based on tropical rainforest removal of 1%/y and Northern forest regrowth of 0.3%/y</doc>
				<eqn>0.004</eqn>
				<units>1/year</units>
			</aux>
			<aux name="FFLReoOGRR">
				<eqn>MAX(1, 1+&quot;sFFLReoOGRR&lt;0&quot;*(Fraction_forestry_land_remaining-Threshold_FFLR))</eqn>
				<units>1</units>
			</aux>
			<aux name="CO2 concentration in atm ppm" access="input">
				<units>ppm</units>
			</aux>
			<aux name="GDP" access="input">
				<units>G$/Years</units>
			</aux>
			<aux name="Cost of energy G$/y" access="input">
				<units>G$/year</units>
			</aux>
			<aux name="GDP per person k$/p/y" access="input">
				<units>k$/(p*year)</units>
			</aux>
			<aux name="Observed warming deg C" access="input">
				<units>deg C</units>
			</aux>
			<aux name="Population Mp" access="input">
				<units>Mp</units>
			</aux>
			<aux name="CO2 concentration in 1980">
				<doc>	410 i 2018 plus 2 ppm/y</doc>
				<eqn>INIT(CO2_concentration_in_atm_ppm)</eqn>
				<units>ppm</units>
			</aux>
			<aux name="Warming effect on\nland yield">
				<eqn>(1 + &quot;sOWeoACY&lt;0&quot; * (Observed_warming_deg_C/Observed_warming_in_1980-1))</eqn>
				<units>1</units>
			</aux>
			<aux name="Unit M$/G$">
				<eqn>1000</eqn>
				<units>M$/G$</units>
			</aux>
			<aux name="UNIT kgN/tN">
				<eqn>1000</eqn>
				<units>kgN/tN</units>
			</aux>
			<aux name="Rate of CO2 absorption in\nforestry land">
				<eqn>1.6</eqn>
				<units>tCO2/(ha*year)</units>
			</aux>
			<aux name="UNIT MtCO2/GtCO2">
				<eqn>1000</eqn>
				<units>MtCO2/GtCO2</units>
			</aux>
			<aux name="UNIT tcrop/t redmeat">
				<eqn>24</eqn>
				<units>(tcrop)/(t redmeat)</units>
			</aux>
			<aux name="UNIT kg red meat/ton read meat">
				<eqn>1000</eqn>
				<units>(kg redmeat)/(t redmeat)</units>
			</aux>
			<aux name="Grazing land yied in\n1980 before multiplication">
				<eqn>14</eqn>
				<units>(kg redmeat)/(ha*year)</units>
			</aux>
			<aux name="UNIT (kg crop)/(t crop)">
				<eqn>1000</eqn>
				<units>(kg crop)/(tcrop)</units>
			</aux>
			<aux name="UNIT kgN/tN 1">
				<eqn>1000</eqn>
				<units>kgN/tN</units>
			</aux>
			<aux name="time for percieved\ncrop balance to impact cropland">
				<eqn>1</eqn>
				<units>year</units>
			</aux>
			<aux name="Observed warming in 1980" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>deg C</units>
			</aux>
			<aux name="CO2 release per ha of new\nforest cut">
				<doc>	Numbers for Northern forests from ESMICON 2016: (215 GtC * 3.7 tCO2/tC ) / 1100 Mha = 72 tCO2/ha
		Numbers for Southern forests from ESMICON 2016: (260 GtC * 3.7 tCO2/tC ) / 1600 Mha = 60 tCO2/ha</doc>
				<eqn>73</eqn>
				<units>tCO2/ha</units>
			</aux>
