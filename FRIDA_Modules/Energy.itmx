	<model name="Energy">
		<variables>
			<aux name="Max non-fossil CO2\nper person">
				<doc>	CO2 from industrial processes was 3.4 GtCO2/y in 2019 i.e. 3.4/7.9=0.43 tCO2/p/y.
		GDPpp=15.000 k$/p/y so max must be higher. In Norway much higher.</doc>
				<eqn>0.5</eqn>
				<units>tCO2/(p*year)</units>
			</aux>
			<aux name="Extra reduction in demand for\nnon-el fossil fuel\nfrom NE">
				<eqn>Fraction_new_electrification*&quot;Demand_for_fossil_fuel_for_non-el_use_before_NE&quot;</eqn>
				<units>Mtoe/year</units>
			</aux>
			<aux name="Demand for fossil fuel for\nnon-el use before NE">
				<eqn>(Population_Mp*&quot;Traditional_per_person_use_of_fossil_fuels_for_non-el-use_before_EE&quot;*EXP(-Normal_increase_in_energy_efficiency*(TIME-1980)))/Extra_energy_productivity_index_2022</eqn>
				<units>Mtoe/year</units>
			</aux>
			<aux name="Desired supply of\nrenewable electricity">
				<eqn>Demand_for_electricity*Desired_renewable_electricity_share</eqn>
				<units>TWh/year</units>
			</aux>
			<aux name="Demand for electricity">
				<eqn>Demand_for_electricity_before_NE*OWeoDFE+Extra_increase_in_demand_for_electricity_from_NE</eqn>
				<units>TWh/year</units>
			</aux>
			<aux name="CO2 emissions per person">
				<eqn>(CO2_from_energy_and_industry/Population_Mp)*&quot;UNIT_G/M&quot;</eqn>
				<units>tCO2/(p*year)</units>
			</aux>
			<aux name="4 TWh-el per Mtoe">
				<eqn>(&quot;TWh-el_per_EJ_-_engineering_equivalent&quot;/&quot;Mtoe_per_EJ_-_calorific_equivalent&quot;)/&quot;UNIT_TWh/(Million*MWh)&quot;</eqn>
				<units>MWh/toe</units>
			</aux>
			<aux name="TWh-el per EJ - engineering equivalent">
				<eqn>&quot;TWh-heat_per_EJ_-_calorific_equivalent&quot;*Efficiency_of_fossil_power_plant</eqn>
				<units>TWh/EJ</units>
			</aux>
			<aux name="Mtoe per EJ - calorific equivalent">
				<doc>	1 toe = 43 GJ. Means that 1Mtoe = 43 MGJ = 43e15 and 24 Mtoe = 1032 e15J = 1.03 e18J = 1 EJ</doc>
				<eqn>24</eqn>
				<units>Mtoe/EJ</units>
			</aux>
			<aux name="Renewable heat\nproduction">
				<eqn>Biomass_energy+&quot;Green_hydrogen_Mtoe/y&quot;</eqn>
				<units>Mtoe/year</units>
			</aux>
			<aux name="Biomass energy">
				<eqn>0</eqn>
				<units>Mtoe/year</units>
			</aux>
			<aux name="Green hydrogen Mtoe/y">
				<eqn>Green_hydrogen*toe_per_tH2</eqn>
				<units>Mtoe/year</units>
			</aux>
			<aux name="IIASA Renewable energy\nproduction">
				<eqn>Renewable_electricity_production/&quot;TWh-el_per_EJ_-_engineering_equivalent&quot;+Renewable_heat_production/&quot;Mtoe_per_EJ_-_calorific_equivalent&quot;</eqn>
				<units>EJ/year</units>
			</aux>
			<aux name="Renewable electricity\nproduction">
				<eqn>Renewable_electricity_capacity*&quot;Renewable_capacity_up-time&quot;</eqn>
				<units>TWh/year</units>
			</aux>
			<aux name="ROC in tCO2 per toe">
				<doc>	CO2 emissions per toe fell from 2.8 in 1980 to 2.4 in 2019 - minus 0.3 %/y - probably because of fuel shift</doc>
				<eqn>-0.00850535148615583</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Efficiency of fossil power\nplant">
				<doc>	1 toe contains 12 MWh-heat and produces ca 4 MWh-el in a modern fossil power plant. 4/12=ca0.345 (chosen to get 4 TWH-el per Mtoe)</doc>
				<eqn>0.345</eqn>
				<units>1</units>
			</aux>
			<aux name="Normal increase in\nenergy efficiency">
				<eqn>0.01</eqn>
				<units>1/year</units>
			</aux>
			<stock name="Extra energy productivity\nindex 2022">
				<eqn>Extra_energy_productivity_index_in_2022</eqn>
				<inflow>Increase_in_extra_energy_productivity_index</inflow>
				<units>1</units>
			</stock>
			<aux name="Demand for fossil fuel\nfor non-el use">
				<eqn>&quot;Demand_for_fossil_fuel_for_non-el_use_before_NE&quot;-&quot;Extra_reduction_in_demand_for_non-el_fossil_fuel_from_NE&quot;</eqn>
				<units>Mtoe/year</units>
			</aux>
			<aux name="Electricity production">
				<eqn>Fossil_electricity_production+Nuclear_electricity_production+Renewable_electricity_production</eqn>
				<units>TWh/year</units>
			</aux>
			<aux name="IIASA Fossil energy\nproduction">
				<eqn>Use_of_fossil_fuels/&quot;Mtoe_per_EJ_-_calorific_equivalent&quot;</eqn>
				<units>EJ/year</units>
			</aux>
			<aux name="TWh-heat per EJ - calorific equivalent">
				<doc>	From 1 J = 1 Ws and 1h = 3600 s</doc>
				<eqn>278</eqn>
				<units>TWh/EJ</units>
			</aux>
			<aux name="Demand for electricity\nbefore NE">
				<eqn>(Population_Mp*Traditional_per_person_use_of_electricity_before_EE*EXP(-Normal_increase_in_energy_efficiency*(TIME-1980)))/Extra_energy_productivity_index_2022</eqn>
				<units>TWh/year</units>
			</aux>
			<aux name="Traditional per person use of\nelectricity before EE">
				<doc>	211023: Set equal to
		([(0,0)-(60,10)],(0,0),(6.1,2.02),(9.3,2.35),(12.8,3.05),(60,10) )
		corrected for 1%/y normal energy efficiency rise</doc>
				<eqn>&quot;GDP_per_person_k$/p/y&quot;</eqn>
				<gf>
					<yscale min="0" max="13"/>
					<xpts>0,10,20,30,50,65</xpts>
					<ypts>0,4,7,9,12,13</ypts>
				</gf>
				<units>MWh/(p*year)</units>
			</aux>
			<aux name="CO2 from energy and\nindustry" access="output">
				<eqn>CO2_from_energy_production+&quot;CO2_from_non-fossil_industrial_processes&quot;</eqn>
				<units>GtCO2/year</units>
			</aux>
			<aux name="Cost of energy" access="output">
				<eqn>&quot;Cost_of_fossil_fuel_for_non-el_use&quot;+Cost_of_electricity+Cost_of_grid+Cost_of_new_electrification+Cost_of_CCS+&quot;Cost_of_air_capture_G$/y&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Cost of fossil fuel\nfor non-el use">
				<eqn>(&quot;Demand_for_fossil_fuel_for_non-el_use&quot;*&quot;Traditional_cost_of_fossil_fuel_for_non-el_use_$/toe&quot;)/&quot;UNIT_G/M&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Cost of electricity">
				<eqn>Cost_of_fossil_electricity+Cost_of_renewable_electricity+Cost_of_nuclear_electricity</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Cost of grid">
				<eqn>(Electricity_production*Transmission_cost)/&quot;UNIT_TWh/(Giga*kWh)&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Cost of new\nelectrification">
				<eqn>(&quot;Extra_cost_per_reduced_use_of_non-el_FF&quot;/&quot;UNIT_G/M&quot;)*&quot;Extra_reduction_in_demand_for_non-el_fossil_fuel_from_NE&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Cost of CCS">
				<eqn>Installed_CCS_capacity*&quot;Cost_of_CCS_$/tCO2&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Cost of CCS $/tCO2" access="output">
				<doc>	DNV-GL assumes cost of CCS at 95 USD/tCO2 in 2020, delining to 70 in 2050 (with limited uptake). 
		This means 85 EUR/tCO2 declining to 63 when 1 USD = 0.9 EUR. Supports Lavutslippsutvalget:
		To make gas power with CCS competitive with conventional gas power requires a carbon price of 50 EUR/tCO2.
		Since each toe emits 2.8 tCO2, the cost of CCS is around 2.8*50 = 150 EUR/toe.</doc>
				<eqn>95</eqn>
				<units>$/tCO2</units>
			</aux>
			<aux name="Installed CCS capacity">
				<eqn>&quot;Fraction_of_CO2-sources_with_CCS&quot;*(&quot;CO2_from_non-fossil_industrial_processes&quot;+CO2_from_energy_production)/(1-&quot;Fraction_of_CO2-sources_with_CCS&quot;)</eqn>
				<units>GtCO2/year</units>
			</aux>
			<aux name="Fraction of CO2-sources\nwith CCS">
				<eqn>&quot;Fraction_of_CO2-sources_with_CCS_in_2022&quot;+RAMP((&quot;Goal_for_fraction_of_CO2-sources_with_CCS&quot;-&quot;Fraction_of_CO2-sources_with_CCS_in_2022&quot;)/Introduction_period_for_policy_y, 2022, 2022+Introduction_period_for_policy_y)</eqn>
				<units>1</units>
			</aux>
			<aux name="CO2 from non-fossil\nindustrial processes">
				<eqn>(&quot;Non-fossil_CO2_per_person&quot;/&quot;UNIT_G/M&quot;)*Population_Mp*(1-&quot;Fraction_of_CO2-sources_with_CCS&quot;)</eqn>
				<units>GtCO2/year</units>
			</aux>
			<aux name="CO2 from energy\nproduction">
				<eqn>Use_of_fossil_fuels*(tCO2_per_toe/&quot;UNIT_G/M&quot;)*(1-&quot;Fraction_of_CO2-sources_with_CCS&quot;)</eqn>
				<units>GtCO2/year</units>
			</aux>
			<aux name="Fraction new\nelectrification">
				<eqn>Fraction_new_electrification_in_1980+RAMP((Fraction_new_electrification_in_2022-Fraction_new_electrification_in_1980)/time_for_RAMP1, 1980, 2022)+RAMP((Goal_for_fraction_new_electrification-Fraction_new_electrification_in_2022)/Introduction_period_for_policy_y, 2022, 2022+Introduction_period_for_policy_y)</eqn>
				<units>1</units>
			</aux>
			<aux name="Fraction new\nelectrification in 1980">
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<aux name="Fraction new\nelectrification in 2022">
				<doc>	was 0.03</doc>
				<eqn>0.03</eqn>
				<units>1</units>
			</aux>
			<aux name="Goal for fraction new\nelectrification">
				<doc>	0.5 i TLTL and 1 in GL</doc>
				<eqn>0.05</eqn>
				<units>1</units>
			</aux>
			<aux name="Fraction of CO2-sources\nwith CCS in 2022">
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<aux name="Goal for fraction of\nCO2-sources with CCS">
				<doc>	0.2 in TLTL and 0.9 in GL
		set to 0.06 which means 6 % of non-el fossil use (5600 Mtoe/y= 15 GtCO2/y in 2050) and non-fossil CO2 industrial processes (16 GtCO2/y) are equipped with CCS. Means capturing ca 2 GtCO2/y or 2000 big CCS plants. Is our estimate of what is assumed in TLTL. Could be achieved by 2100.</doc>
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<aux name="Desired renewable\nelectricity share">
				<eqn>Renewable_el_fraction_in_1980+RAMP((Renewable_el_fraction_in_2022-Renewable_el_fraction_in_1980)/time_for_ramp, 1980, 2022)+RAMP((Goal_for_renewable_el_fraction-Renewable_el_fraction_in_2022)/Introduction_period_for_policy_y, 2022, 2022+Introduction_period_for_policy_y)</eqn>
				<units>1</units>
			</aux>
			<aux name="Renewable el\nfraction in 1980">
				<eqn>0.065</eqn>
				<units>1</units>
			</aux>
			<aux name="Renewable el\nfraction in 2022">
				<eqn>0.36017328814822</eqn>
				<units>1</units>
			</aux>
			<aux name="Goal for renewable\nel fraction">
				<doc>	0.5 i TLTL and 1 in GL</doc>
				<eqn>0.45</eqn>
				<units>1</units>
			</aux>
			<aux name="Non-fossil CO2 per\nperson">
				<eqn>&quot;Max_non-fossil_CO2_per_person&quot;*(1-EXP(-(&quot;GDP_per_person_k$/p/y&quot;/10)))</eqn>
				<units>tCO2/(p*year)</units>
			</aux>
			<aux name="Use of fossil fuels">
				<eqn>&quot;Demand_for_fossil_fuel_for_non-el_use&quot;+Fossil_fuels_for_electricity</eqn>
				<units>Mtoe/year</units>
			</aux>
			<aux name="tCO2 per toe">
				<doc>	CO2 emissions per toe fell from 2.8 in 1980 to 2.4 in 2019 - minus 0.3 %/y - probably because of fuel shift</doc>
				<eqn>tCO2_per_toe_in_1980*EXP(ROC_in_tCO2_per_toe*(TIME-1980))</eqn>
				<units>tCO2/toe</units>
			</aux>
			<aux name="Energy use per person">
				<eqn>ENERGY_USE/Population_Mp</eqn>
				<units>toe/(p*year)</units>
			</aux>
			<aux name="ENERGY USE">
				<eqn>&quot;Demand_for_fossil_fuel_for_non-el_use&quot;+Electricity_production/&quot;4_TWh-el_per_Mtoe&quot;+Renewable_heat_production</eqn>
				<units>Mtoe/year</units>
			</aux>
			<aux name="Extra cost of Energy\nTurnaround as share of GDP">
				<eqn>( IF TIME &gt; 2022 THEN (Cost_of_energy-Traditional_cost_of_energy)/GDP ELSE 0 )</eqn>
				<units>1</units>
			</aux>
			<aux name="Traditional cost of energy">
				<eqn>Traditional_cost_of_electricity+&quot;Traditional_cost_of_fossil_fuel_for_non-el_use&quot;+Traditional_grid_cost</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Ratio of Energy cost to\nTrad energy cost">
				<eqn>Cost_of_energy/Traditional_cost_of_energy</eqn>
				<units>1</units>
			</aux>
			<aux name="Traditional cost of electricity $/kWh">
				<doc>	80% fossil at 0.03, 15 % nuclear at 0.033, 5 % hydro at 0.025</doc>
				<eqn>0.03</eqn>
				<units>$/kWh</units>
			</aux>
			<aux name="Traditional cost of\nelectricity">
				<eqn>(Demand_for_electricity_before_NE*&quot;Traditional_cost_of_electricity_$/kWh&quot;/&quot;UNIT_TWh/(Giga*kWh)&quot;)*&quot;Adjustment_factor_to_make_costs_match_1980-2022&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Adjustment factor to make\ncosts match 1980-2022">
				<eqn>1.35</eqn>
				<units>1</units>
			</aux>
			<aux name="Traditional cost of energy\nas share of GDP">
				<eqn>Traditional_cost_of_energy/GDP</eqn>
				<units>1</units>
			</aux>
			<aux name="Traditional cost of fossil fuel for non-el use $/toe">
				<doc>	Cost per ton is cost per barrel times 6.
		33% coal at 30*6=180, 33 % oil at 50*6=300, 33 % gas at 40*6=240</doc>
				<eqn>240</eqn>
				<units>$/toe</units>
			</aux>
			<flow name="Addition of sun and\nwind capacity">
				<eqn>Addition_of_renewable_el_capacity</eqn>
				<units>GW/Year</units>
			</flow>
			<flow name="Addition of renewable\nel capacity">
				<eqn>MAX(0, (Desired_renewable_el_capacity_change/Renewable_el_construction_time)+(Discard_of_renewable_el_capacity))</eqn>
				<units>GW/Year</units>
			</flow>
			<aux name="Transmission cost">
				<doc>	DNV-GL ETO 2018 p 176 says 0.02</doc>
				<eqn>0.02</eqn>
				<units>$/kWh</units>
			</aux>
			<aux name="Traditional grid cost">
				<eqn>((Demand_for_electricity_before_NE*Transmission_cost)*&quot;UNIT_kWh/MWh&quot;)/&quot;UNIT_G/M&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Traditional cost of fossil\nfuel for non-el use">
				<eqn>(&quot;Demand_for_fossil_fuel_for_non-el_use_before_NE&quot;*&quot;Traditional_cost_of_fossil_fuel_for_non-el_use_$/toe&quot;/&quot;UNIT_G/M&quot;)*&quot;Adjustment_factor_to_make_costs_match_1980-2022&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Cost of nuclear\nelectricity">
				<eqn>(Nuclear_electricity_production*Cost_of_nuclear_el)/&quot;UNIT_TWh/(Giga*kWh)&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Nuclear electricity\nproduction">
				<eqn>Nuclear_capacity*&quot;Nuclear_capacity_up-time&quot;</eqn>
				<units>TWh/Years</units>
			</aux>
			<aux name="Cost of nuclear el">
				<doc>	I assume nuclear el is 10 % more expensive than fossil el</doc>
				<eqn>0.033</eqn>
				<units>$/kWh</units>
			</aux>
