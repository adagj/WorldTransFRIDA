			</stock>
			<aux name="UNIT G/(M*k)" access="input">
				<eqn>1</eqn>
				<units>G/(M*k)</units>
			</aux>
			<aux name="Govmnt borrowing cost">
				<eqn>0.0123106728476454</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Worker borrowing cost">
				<eqn>0.04</eqn>
				<units>1/year</units>
			</aux>
			<aux name="WSO in 1980" access="input">
				<eqn>0.55</eqn>
				<units>1</units>
			</aux>
			<aux name="Workforce" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>Mp</units>
			</aux>
			<aux name="Investment in new\ncapacity PIS" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="GDP 1" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>G$/Years</units>
