	<model name="Public_sector">
		<variables>
			<aux name="Public spending\nper person" access="output">
				<eqn>(&quot;Govmnt_spending_G$/y&quot;/Population_Mp)/&quot;UNIT_G/(M*k)&quot;</eqn>
				<units>k$/p/year</units>
			</aux>
			<aux name="sIIEeoROTA&lt;0">
				<eqn>-0.1</eqn>
				<units>1</units>
			</aux>
			<aux name="rate of change\nin TFP">
				<eqn>(normal_rate_of_change_in_TFP)*Reduction_in_ROTA_from_inequality*OWeoROTA</eqn>
				<units>1/year</units>
			</aux>
			<aux name="normal\nrate of change\nin TFP">
				<eqn>0.0310410147566926</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Reduction in ROTA\nfrom inequality">
				<eqn>MIN(1, 1+&quot;sIIEeoROTA&lt;0&quot;*(&quot;INEQUALITY_INDEX_(1980=1)&quot;/1-1))</eqn>
				<units>1</units>
			</aux>
			<aux name="OWeoROTA">
				<eqn>1+&quot;sOWeoTFP&lt;0&quot;*(Observed_warming_deg_C/Observed_warming_in_1980-1)</eqn>
				<units>1</units>
			</aux>
			<aux name="sOWeoTFP&lt;0">
				<doc>	220323 Set to - 0.1 to get TFP reduced by 10 % at +2.5 deg C. (dvs -4% per deg C)</doc>
				<eqn>-0.26</eqn>
				<units>1</units>
			</aux>
			<flow name="Change in TFP">
				<eqn>rate_of_change_in_TFP*Indicated_TFP</eqn>
				<units>Per Year</units>
			</flow>
			<aux name="TFP in 1980">
				<eqn>1</eqn>
				<units>1</units>
			</aux>
			<stock name="Indicated TFP" access="output">
				<eqn>TFP_in_1980</eqn>
				<inflow>Change_in_TFP</inflow>
				<units>1</units>
			</stock>
			<aux name="Govmnt spending G$/y" access="input">
				<units>G$/year</units>
			</aux>
			<aux name="INEQUALITY INDEX (1980=1)" access="input">
				<units>1</units>
			</aux>
			<aux name="Observed warming deg C" access="input">
				<units>deg C</units>
			</aux>
			<aux name="UNIT G/(M*k)" access="input">
				<eqn>1</eqn>
				<units>G/(M*k)</units>
			</aux>
			<aux name="Observed warming in 1980" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>deg C</units>
			</aux>
			<aux name="Population Mp" access="input">
				<units>Mp</units>
			</aux>
		</variables>
