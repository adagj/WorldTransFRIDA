			</aux>
			<aux name="life of public capital 1980">
				<eqn>15</eqn>
				<units>year</units>
			</aux>
			<aux name="investment share of GDP">
				<eqn>(investment_in_private_capital+&quot;Govmnt_investment_in_public_capacity_G$/y&quot;)/GDP</eqn>
				<units>1</units>
			</aux>
			<aux name="investment in private\ncapital">
				<eqn>available_investment_capital*fraction_of_available_capital_for_investment</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="life of public capital">
				<eqn>life_of_public_capital_1980*effect_of_surface_temperature_anomaly_on_life_of_capacity</eqn>
				<units>year</units>
			</aux>
			<flow name="change in total factor\nproductivity">
				<eqn>(Indicated_Total_Factor_Productivity-Total_Factor_Productivity)*capital_replacement_renewal_rate</eqn>
				<units>Per Year</units>
			</flow>
			<aux name="capital replacement renewal rate">
				<eqn>private_capital_finishing_construction/Private_Capital</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Total Factor\nProductivity 1980">
				<eqn>1</eqn>
				<units>1</units>
			</aux>
			<flow name="private capital finishing\nconstruction">
				<eqn>Private_Capital_Under_Construction/private_capital_construction_time</eqn>
				<units>G$/Years</units>
			</flow>
			<aux name="GDP in 1980">
				<eqn>Private_Capital_1980/private_capital_productivity_1980+Public_Capital_1980/public_capital_productivity_1980</eqn>
				<units>G$/Years</units>
			</aux>
			<aux name="investment planning time">
				<eqn>5</eqn>
				<units>year</units>
			</aux>
			<aux name="private capital\nconstruction time">
				<eqn>1.5</eqn>
				<units>year</units>
			</aux>
			<aux name="fraction of available capital\nfor investment">
				<eqn>SMTH1(MIN(1, normal_fraction_of_savings_for_investment*effect_of_GDP_per_person_on_fraction_of_available_capital_for_investment), investment_planning_time)</eqn>
				<isee:delay_aux/>
				<units>1</units>
			</aux>
			<aux name="normal fraction\nof savings for investment">
				<eqn>1</eqn>
				<units>1</units>
			</aux>
			<aux name="effect of GDP per person\non fraction of available capital\nfor investment">
				<eqn>1+sensitivity_of_GDP_per_person_on_fraction_of_available_capital_for_investment*(&quot;GDP_per_person_k$/p/y&quot;/GDP_per_person_in_1980-1)</eqn>
				<units>1</units>
			</aux>
			<aux name="sensitivity of GDP per person\non fraction of available capital\nfor investment">
				<eqn>-0.034</eqn>
				<units>1</units>
			</aux>
			<aux name="GDP" access="output">
				<eqn>GDP_in_1980*(((Private_Capital+Public_Capital)/(Private_Capital_1980+Public_Capital_1980))^Kaper_persona)*((&quot;Labour_use_Gph/y&quot;/&quot;Labour_use_in_1980_Gph/y&quot;)^Lambda)*(Total_Factor_Productivity)</eqn>
				<units>G$/Years</units>
			</aux>
			<stock name="Private Capital">
				<eqn>Private_Capital_1980</eqn>
				<inflow>private_capital_finishing_construction</inflow>
				<outflow>private_capital_depreciation</outflow>
				<units>G$</units>
			</stock>
			<stock name="Public Capital">
				<eqn>Public_Capital_1980</eqn>
				<inflow>public_capital_finishing_construction</inflow>
				<outflow>public_capital_depreciation</outflow>
				<units>G$</units>
			</stock>
			<aux name="Private Capital 1980">
				<eqn>64794.8236136508</eqn>
				<units>G$</units>
			</aux>
			<aux name="Public Capital\n1980">
				<eqn>488.945222051569</eqn>
				<units>G$</units>
			</aux>
			<aux name="Kaper persona">
				<eqn>0.3</eqn>
				<units>1</units>
			</aux>
			<aux name="Lambda">
				<eqn>1-Kaper_persona</eqn>
				<units>1</units>
			</aux>
			<stock name="Total Factor Productivity">
