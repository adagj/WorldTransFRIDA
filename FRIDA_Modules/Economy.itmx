				<eqn>0</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="government investment\ncorruption fraction">
				<eqn>0.075</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="corrupt consumption">
				<eqn>Consumption_demand*corrupt_consumption_fraction</eqn>
				<units>G$/Years</units>
			</aux>
			<aux name="corrupt investment">
				<eqn>Full_govmnt_investment_in_public_capacity*government_investment_corruption_fraction</eqn>
				<units>G$/Years</units>
			</aux>
			<aux name="Full govmnt investment\nin public capacity">
				<eqn>(Permanent_govmnt_cash_inflow-Govmnt_purchases)</eqn>
				<units>G$/Years</units>
			</aux>
			<aux name="total corruption">
				<eqn>corrupt_investment+corrupt_consumption</eqn>
				<units>G$/Years</units>
			</aux>
			<aux name="corruption share\nof GDP">
				<eqn>total_corruption/GDP</eqn>
				<units>dmnl</units>
			</aux>
