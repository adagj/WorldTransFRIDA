				<eqn>15000</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Govmnt interest paid">
				<eqn>MIN(Govmnt_net_income*govmnt_max_interest_threshold, Govmnt_interest_cost)</eqn>
				<units>G$/Years</units>
			</aux>
