				<units>k$/(p*year)</units>
			</aux>
			<aux name="GDP per person in 1980 k$/p/y" access="input">
				<units>k$/(p*year)</units>
			</aux>
			<aux name="GDP" access="output" throughput="true" isee:autocreated="true">
				<units>G$/Years</units>
