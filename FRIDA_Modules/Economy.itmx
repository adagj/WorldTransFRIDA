	</model>
	<model name="Demand">
		<variables>
			<aux name="Inequality" access="output">
				<eqn>Owner_operating_income_after_tax/&quot;Worker_income_after_tax_G$/y&quot;</eqn>
				<units>1</units>
			</aux>
			<aux name="Govmnt spending" access="output">
				<eqn>Govmnt_purchases+Govmnt_investment_in_public_capacity</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Worker disposable income" access="output">
				<eqn>(Permanent_worker_cash_inflow/Workforce)/&quot;UNIT_G/(M*k)&quot;</eqn>
				<units>k$/p/year</units>
			</aux>
			<aux name="Govmnt payback period">
				<doc>	was 100, so 1000 (did not matter),</doc>
				<eqn>200</eqn>
				<units>year</units>
			</aux>
			<aux name="Max govmnt debt burden">
				<eqn>1</eqn>
				<units>year</units>
			</aux>
			<aux name="Govmnt interest cost">
				<eqn>Govmnt_debt*Govmnt_borrowing_cost</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Worker income after tax G$/y">
				<eqn>&quot;Worker_income_G$/y&quot;-Worker_taxes+Transfer_payments</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="INEQUALITY INDEX (1980=1)" access="output">
				<doc>	Inequity is defined as &quot;owners income after tax&quot; divided by &quot;workers imcome after tax&quot;.</doc>
				<eqn>Inequality/Inequality_in_1980</eqn>
				<units>1</units>
			</aux>
			<aux name="Inequality in 1980">
				<doc>	=8426/13874</doc>
				<eqn>INIT(Inequality)</eqn>
				<units>1</units>
			</aux>
			<aux name="Time to implement new taxes">
				<eqn>5</eqn>
				<units>year</units>
			</aux>
			<aux name="Owner taxes">
				<eqn>Income_tax_owners+Extra_taxes_from_2022*Fraction_of_extra_taxes_paid_by_owners</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Income tax owners">
				<eqn>Basic_income_tax_rate_owners*GDP*(1-&quot;Worker_share_of_output_(1)&quot;)</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Extra taxes from 2022">
				<eqn>SMTH1(Goal_for_extra_taxes_from_2022, Time_to_implement_new_taxes)</eqn>
				<isee:delay_aux/>
				<units>G$/year</units>
			</aux>
			<aux name="Fraction of extra taxes\npaid by owners">
				<doc>	0.5 in TLTL and 0.8 in GL</doc>
				<eqn>0.5</eqn>
				<units>1</units>
			</aux>
			<aux name="Goal for extra\ntaxes from 2022">
				<eqn>Extra_general_tax_from_2022</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Owner operating\nincome after tax">
				<eqn>Owner_income-Owner_taxes</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Owner income">
				<eqn>GDP*(1-&quot;Worker_share_of_output_(1)&quot;)</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Fraction of govmnt\nbudget to workers">
				<eqn>SMTH1(Goal_for_fraction_of_govmnt_budget_to_workers, Time_to_implement_new_taxes)</eqn>
				<isee:delay_aux/>
				<units>1</units>
			</aux>
			<aux name="Goal for fraction of govmnt\nbudget to workers">
				<eqn>Fraction_transferred_in_1980+( IF TIME &gt; 2022 THEN Extra_transfer_of_govmnt_budget_to_workers ELSE 0 )</eqn>
				<units>1</units>
			</aux>
			<aux name="Govmnt gross income">
				<eqn>Worker_taxes+Owner_taxes+Sales_tax_workers+Sales_tax_owners+Income_from_commons_from_2022</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Worker taxes">
				<eqn>Income_tax_workers+Extra_taxes_from_2022*(1-Fraction_of_extra_taxes_paid_by_owners)</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Sales tax workers">
				<eqn>Worker_consumption_demand*Sales_tax_rate</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Sales tax owners">
				<eqn>Owner_consumption*Sales_tax_rate</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Income from commons\nfrom 2022">
				<eqn>GDP*( IF TIME &gt; 2022 THEN RAMP(&quot;Goal_for_extra_income_from_commons_(share_of_NI)&quot;/Introduction_period_for_policy_y, 2022, 2020+Introduction_period_for_policy_y) ELSE 0 )</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Transfer payments">
				<eqn>Govmnt_gross_income*Fraction_of_govmnt_budget_to_workers</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Owner tax rate">
				<eqn>Owner_taxes/Owner_income</eqn>
				<units>1</units>
			</aux>
			<aux name="Income tax workers">
				<eqn>Basic_income_tax_rate_workers*GDP*&quot;Worker_share_of_output_(1)&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Govmnt gross income (as share of NI)">
				<eqn>Govmnt_gross_income/GDP</eqn>
				<units>1</units>
			</aux>
			<aux name="Fraction transferred\nin 1980">
				<doc>	Logic: 0.3 is value for US 1980 to 2020 (8% of GDP transferred of budget 28% of GDP = 0.29. Ca 0.5 in Norway</doc>
				<eqn>0.3</eqn>
				<units>1</units>
			</aux>
			<aux name="Extra transfer of govmnt\nbudget to workers">
				<doc>	0 in TLTL and 0.2 in GL
		on top of 0.3 in all runs</doc>
				<eqn>0.00211893046349465</eqn>
				<units>1</units>
			</aux>
			<aux name="Goal for extra income from commons (share of NI)">
				<doc>	0 i TLTL and 0.02 in GL</doc>
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<aux name="Extra pension tax from\n2022 (share of NI)">
				<doc>	0 in TLTL and 0.02 in GL</doc>
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<aux name="Extra general tax\nfrom 2022">
				<eqn>( IF TIME &gt; 2022 THEN Extra_general_tax_rate_from_2022+&quot;Extra_empowerment_tax_from_2022_(share_of_NI)&quot;+&quot;Extra_pension_tax_from_2022_(share_of_NI)&quot; ELSE 0 )*GDP</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Extra general tax\nrate from 2022">
				<doc>	0 in TLTL and 0.01 in GL</doc>
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<aux name="Extra empowerment tax\nfrom 2022 (share of NI)">
				<doc>	0 in TLTL and 0.02 in GL</doc>
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<aux name="Worker tax rate">
				<eqn>Worker_taxes/&quot;Worker_income_G$/y&quot;</eqn>
				<units>1</units>
			</aux>
			<aux name="Worker income G$/y">
				<eqn>GDP*&quot;Worker_share_of_output_(1)&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<flow name="Cancellation of debt">
				<eqn>Govmnt_interest_cost-Govmnt_interest_paid</eqn>
				<units>G$/Year</units>
			</flow>
			<stock name="Govmnt debt">
				<eqn>Govmnt_debt_in_1980</eqn>
				<inflow>Govmnt_new_debt</inflow>
				<outflow>Cancellation_of_debt</outflow>
				<outflow>Govmnt_payback</outflow>
				<units>G$</units>
			</stock>
			<aux name="Govmnt investment in\npublic capacity" access="output">
				<eqn>Full_govmnt_investment_in_public_capacity*(1-government_investment_corruption_fraction)</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Basic income tax\nrate owners">
				<eqn>0.287476182483638</eqn>
				<units>1</units>
			</aux>
			<aux name="Basic income tax\nrate workers">
				<doc>	211125 changed from 0.1 to mimic more typical countries than US</doc>
				<eqn>0.0785785430208779</eqn>
				<units>1</units>
			</aux>
			<flow name="Govmnt new debt">
				<eqn>MAX(0, (Max_govmnt_debt-Govmnt_debt)/Govmnt_drawdown_period)</eqn>
				<units>G$/Year</units>
			</flow>
			<flow name="Govmnt payback">
				<eqn>MIN(Govmnt_debt/Govmnt_payback_period,Govmnt_net_income*govmnt_max_interest_threshold)</eqn>
				<units>G$/Year</units>
			</flow>
			<aux name="Govmnt debt in 1980">
				<doc>	was 1500
		was 28160
		Equal to one year&apos;s national income 28160, somewhat reduced to avoid transient</doc>
				<eqn>18000</eqn>
				<units>G$</units>
			</aux>
			<aux name="Owner cash inflow">
				<eqn>Owner_operating_income_after_tax</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Max govmnt debt">
				<eqn>GDP*Max_govmnt_debt_burden</eqn>
				<units>G$</units>
			</aux>
			<aux name="Govmnt drawdown period">
				<doc>	was 2</doc>
				<eqn>10</eqn>
				<units>year</units>
			</aux>
			<aux name="Bank cash inflow\nas share of NI">
				<eqn>Bank_cash_inflow_from_lending/GDP</eqn>
				<units>1</units>
			</aux>
			<aux name="Bank cash inflow\nfrom lending">
				<eqn>Cash_flow_from_workers_to_banks+Cash_flow_from_govmnt_to_banks</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Govmnt net income\nas share of NI">
				<eqn>Govmnt_net_income/GDP</eqn>
				<units>1</units>
			</aux>
			<aux name="Govmnt net income">
				<eqn>Govmnt_gross_income-Transfer_payments+Sales_tax</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Sales tax">
				<eqn>Sales_tax_workers+Sales_tax_owners</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Govmnt share of GDP">
				<eqn>Govmnt_spending/GDP</eqn>
				<units>1</units>
			</aux>
			<aux name="Consumption share of GDP">
				<eqn>(Consumption_demand*(1-corrupt_consumption_fraction))/GDP</eqn>
				<units>1</units>
			</aux>
			<aux name="Permanent worker\ncash inflow">
				<eqn>SMTH1(Worker_cash_inflow, Time_to_adjust_worker_consumption, WFI_in_1980)</eqn>
				<isee:delay_aux/>
				<units>G$/year</units>
			</aux>
			<aux name="Govmnt purchases" access="output">
				<eqn>Permanent_govmnt_cash_inflow*Government_consumption_fraction</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Owner savings fraction">
				<eqn>MIN(1, Owner_savings_fraction_in_1980*(1+sGDPeoOSR*(&quot;Effective_GDP_per_person_k$/p/y&quot;/&quot;GDP_per_person_in_1980_k$/p/y&quot;-1)))</eqn>
				<units>1</units>
			</aux>
			<aux name="Owner savings fraction in 1980">
				<doc>	was 0.7
		220112 changed from 0.9 to 0.7 for China
		211216 changed back from 0.9 to 0.8 to give reasonable growth in GDP 1980 to 2020 after intro of inequity
		211126 changed from 0.8 to get total savings rate above 0.3
		was 0.66 so 0.9, 0.75, so 0.72, so 0.64</doc>
				<eqn>0.718511361551339</eqn>
				<units>1</units>
			</aux>
			<aux name="sGDPeoOSR">
				<doc>	was 0
		220112 changed from -0.13 to 0 to prove that declinig growth rate with rising GDPpp is not caused by savings rate
		2112nn changed to -0.05 to avoid end of capitalism
		211205 changed from 0.055 to -0.6 to match DC data correctly
		211126 changed from -0.17 to fit DC f(GDPpp)
		was -0.07, so -0.15, so -0.14, so -0.05</doc>
				<eqn>0.043948390729909</eqn>
				<units>1</units>
			</aux>
			<aux name="Savings share of GDP">
				<eqn>Total_savings/GDP</eqn>
				<units>1</units>
			</aux>
			<aux name="Consumption per person">
				<eqn>(Consumption_demand/Population_Mp)/&quot;UNIT_G/(M*k)&quot;</eqn>
				<units>k$/p/year</units>
			</aux>
			<aux name="Consumption demand">
				<eqn>Worker_consumption_demand-Sales_tax_workers+Owner_consumption-Sales_tax_owners</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Total available cash" access="output">
				<eqn>Worker_cash_inflow+Govmnt_cash_inflow+Owner_cash_inflow</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Permanent govmnt\ncash inflow">
				<eqn>SMTH1(Govmnt_cash_inflow, Time_to_adjust_budget)</eqn>
				<isee:delay_aux/>
				<units>G$/year</units>
			</aux>
			<aux name="Govmnt cash inflow">
				<eqn>Govmnt_net_income-Cash_flow_from_govmnt_to_banks</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Time to adjust budget">
				<eqn>1</eqn>
				<units>year</units>
			</aux>
			<aux name="Permanent owner\ncash inflow">
				<eqn>SMTH1(Owner_cash_inflow, Time_to_adjust_owner_consumption)</eqn>
				<isee:delay_aux/>
				<units>G$/year</units>
			</aux>
			<aux name="Time to adjust\nowner consumption">
				<eqn>1</eqn>
				<units>year</units>
			</aux>
			<aux name="Worker cash inflow">
				<eqn>&quot;Worker_income_after_tax_G$/y&quot;-Cash_flow_from_workers_to_banks</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Worker debt burden">
				<eqn>Workers_debt/&quot;Worker_income_after_tax_G$/y&quot;</eqn>
				<units>year</units>
			</aux>
			<stock name="Workers debt">
				<eqn>Workers_debt_in_1980</eqn>
				<inflow>Workers_new_debt</inflow>
				<outflow>Workers_payback</outflow>
				<units>G$</units>
			</stock>
			<aux name="Time to adjust\nworker consumption">
				<eqn>1</eqn>
				<units>year</units>
			</aux>
			<aux name="Worker finance cost as\nshare of income">
				<eqn>Cash_flow_from_workers_to_banks/&quot;Worker_income_after_tax_G$/y&quot;</eqn>
				<units>1</units>
			</aux>
			<aux name="Cash flow from\nworkers to banks">
				<eqn>Worker_interest_cost+Workers_payback-Workers_new_debt</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Owner consumption">
				<eqn>Permanent_owner_cash_inflow*Owner_consumptin_fraction</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Owner consumptin fraction">
				<eqn>1-Owner_savings_fraction</eqn>
				<units>1</units>
			</aux>
			<aux name="Total savings" access="output">
				<eqn>Owner_savings+Worker_savings</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Max workers debt burden">
				<eqn>1</eqn>
				<units>year</units>
			</aux>
			<aux name="Max workers debt">
				<eqn>&quot;Worker_income_G$/y&quot;*Max_workers_debt_burden</eqn>
				<units>G$</units>
			</aux>
			<aux name="Govmnt finance as\nshare of NI">
				<eqn>(Govmnt_interest_cost+Govmnt_payback)/GDP</eqn>
				<units>1</units>
			</aux>
			<aux name="Owner savings">
				<eqn>Permanent_owner_cash_inflow-Owner_consumption</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Govmnt debt burden">
				<eqn>Govmnt_debt/GDP</eqn>
				<units>year</units>
			</aux>
			<aux name="Workers debt in 1980">
				<doc>	was 1062
		was 18992
		equal to one year&apos;s income after tax 22140, somewhat reduced to avoid initial transient</doc>
				<eqn>7400</eqn>
				<units>G$</units>
			</aux>
			<aux name="Worker interest cost">
				<eqn>Workers_debt*Worker_borrowing_cost</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Cash flow from\ngovmnt to banks">
				<eqn>Govmnt_interest_paid+Govmnt_payback-Govmnt_new_debt</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Sales tax rate">
				<doc>	In US 1980-2020 3%. Much higher in countries with VAT around 20 %
		In PUS=0.22 was 0.08</doc>
				<eqn>0.0219028158401505</eqn>
				<units>1</units>
			</aux>
			<aux name="Worker consumption demand">
				<eqn>Permanent_worker_cash_inflow*Worker_consumption_fraction</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Worker consumption fraction">
				<doc>	was 0.6
		220112 changed from 0.85 to 0.6 for China
		was 0.95
		
		211126 changed from 0.95 to get total savings above 0.3
		was 0.95, so 0.8</doc>
				<eqn>0.95</eqn>
				<units>1</units>
			</aux>
			<aux name="Government consumption\nfraction">
				<doc>	was 0.85</doc>
				<eqn>0.974597594073129</eqn>
				<units>1</units>
			</aux>
			<aux name="Worker savings">
				<eqn>Permanent_worker_cash_inflow-Worker_consumption_demand</eqn>
				<units>G$/year</units>
			</aux>
			<flow name="Workers payback">
				<eqn>Workers_debt/Workers_payback_period</eqn>
				<units>G$/Year</units>
			</flow>
			<flow name="Workers new debt">
				<eqn>MAX(0, (Max_workers_debt-Workers_debt)/Workers_drawdown_period)</eqn>
				<units>G$/Year</units>
			</flow>
			<aux name="Workers drawdown period">
				<doc>	Was 2</doc>
				<eqn>10</eqn>
				<units>year</units>
			</aux>
			<aux name="Workers payback period">
				<eqn>20</eqn>
				<units>year</units>
			</aux>
