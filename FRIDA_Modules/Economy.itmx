	<model name="Supply">
		<variables>
			<aux name="life of private capital">
				<eqn>life_of_private_capital_1980*effect_of_surface_temperature_anomaly_on_life_of_capacity</eqn>
				<units>year</units>
			</aux>
			<aux name="life of private capital 1980">
				<eqn>15</eqn>
